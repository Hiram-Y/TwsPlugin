<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wxsdklibrary"
    android:sharedUserId="com.example.pluginhost"
    android:versionCode="1"
    android:versionName="1.0.1" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.WithActionBar" >
        <uses-library
            android:name="com.example.plugintestbase"
            android:required="true" />

        <activity android:name="com.example.wxsdklibrary.TestSendToWXActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.pluginhost.wxapi.WXEntryActivity" />
    </application>

</manifest>